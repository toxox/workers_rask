<%= form_tag(events_path, method: "get") do %>
  <p>
    <%= label_tag(:start, "Start date:") %>
    <%= select_datetime(@start_date || Time.now, prefix: :start_date) %>
  </p>
  <p>
    <%= label_tag(:end, "End date:") %>
    <%= select_datetime(@end_date || Time.now, prefix: :end_date) %>
  </p>
  <%= submit_tag("Filter") %>
<% end %>

<%= line_chart @lines %>
